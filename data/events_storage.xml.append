<FTL>

  <!--///////////////////////////////
  ////////////// STORAGE CHECK OPTIONS ///////////////
  ////////////////////////////////-->

  <mod:findName type="event" name="STORAGE_CHECK_AUG">

    <mod-append:choice req="SCRAP_TRANSMITTER">
      <text>Contact the Trash Compactor.</text>
      <event load="SUMMON_TC_STORAGE"/>
    </mod-append:choice>

    <mod-append:choice req="COMPACTOR">
      <text>(Compactor) Print a custom flak weapon.</text>
      <event load="FLAK_FORGE"/>
    </mod-append:choice>

    <mod-append:choice req="AUTOMATED_QUARTERMASTER">
      <text>(Automated Quartermaster) Print a custom weapon.</text>
      <event load="AUTOMATED_QUARTERMASTER_CHOICE"/>
    </mod-append:choice>

    <mod-append:choice req="SCRAP_COMPACTOR">
      <text>(Scrap Compactor) Print a custom flak weapon.</text>
      <event>
        <text>You start the process...</text>
        <item_modify>
          <item type="scrap" min="-50" max="-50"/>
        </item_modify>
        <choice>
          <text>Continue...</text>
          <event load="SCRAP_COMPACT"/>
        </choice>
      </event>
    </mod-append:choice>

    <mod-append:choice req="FUEL_RESERVOIR">
      <text>(Fuel Reservoir) Create a random 1~10 fuel.</text>
      <event load="FUEL_RESERVOIR_RANDOM"/>
    </mod-append:choice>

    <!-- Move other menus example options to the bottom -->
    <mod:findWithChildLike type="choice" child-type="text">
      <mod:selector>Nevermind, do something else.</mod:selector>
      <mod:setAttributes req="pilot" lvl="1" max_group="998" blue="false"/>
    </mod:findWithChildLike>

  </mod:findName>
  
    <!--/////////////////CUSTOM INTERNAL UPGRADES//////////////////-->
	
	
<mod:findName type="event" name="STORAGE_CHECK_INVENTORY_UPGRADES">
	<mod-append:choice req="TC_CLONEBAY_NOLOSS" lvl="1" blue="false">
		<text>Backup Memory Archive[Already Installed]</text>
		<event load="OPTION_INVALID"/>
	</mod-append:choice>
</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_SYSTEM_TELEPORTER">
	<mod-append:choice req="LIST_ELEMENTAL_SCRAPPER_ALL" lvl="1" max_group="9" blue="true">
		<text>Multiversal Teleporter Upgrades</text>
		<event load="STORAGE_CHECK_SYSTEM_TELEPORTER_MULTIVERSAL"/>
	</mod-append:choice>
</mod:findName>  
<mod:findName type="event" name="STORAGE_CHECK_SYSTEM_CLONEBAY">
	<mod-append:choice req="UPG_TC_CLONEBAY_NOLOSS" lvl="1" max_group="2" blue="false" hidden="true">
		<text>Backup Memory Archive. [ALREADY INSTALLED]</text>
		<event load="OPTION_INVALID"/>
	</mod-append:choice>
	
	<mod-append:choice req="UPG_TC_CLONEBAY_NOLOSS" lvl="0" max_group="2" blue="false" hidden="true">
		<text>Backup Memory Archive. [Cost: 50~]</text>
		<event load="TC_CLONEBAY_NOLOSS_INSTALL"/>
	</mod-append:choice>
</mod:findName>  

<event name="TC_CLONEBAY_NOLOSS_INSTALL">
	<text>You are about to install Backup Memory Archive.
	[Effects: Crew will not suffer skill loss when cloned.]</text>
	<choice req="clonebay" lvl="1" max_lvl="4" max_group="0" blue="false" hidden="true">
		<text>Perform the upgrade. [Cost: 50~]</text>
		<event>
			<text>You install the upgrade.</text>
			<item_modify>
				<item type="scrap" min="-50" max="-50" />
			</item_modify>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_CLONEBAY_NOLOSS" />
			</choice>
		</event>
	</choice>
	<choice req="EX_TC_CLONEBAY_NOLOSS" max_group="2" blue="false" hidden="true">
		<text>Install the External Augment internally.</text>
		<event>
			<text>You install the upgrade.</text>
			<remove name="EX_TC_CLONEBAY_NOLOSS"/>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_CLONEBAY_NOLOSS"/>
			</choice>
		</event>
	</choice>
	<choice req="pilot" lvl="1" max_group="3" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>
	<choice req="LIST_CREW_SCRAPPER" lvl="1" max_group="3"  blue="false" hidden="true">
		<text>Your scrappers can install this upgrade for a reduced cost. [Cost: 35~]</text>
		<event load="SCRAPPER_INSTALL_CLONEBAY_NOLOSS"/>
	</choice>
</event>
<event name="SCRAPPER_INSTALL_CLONEBAY_NOLOSS">
	<text>Your scrappers can install this upgrade for a reduced cost. [Cost: 35~]</text>
	<choice req="clonebay" lvl="1" blue="false" hidden="true">
		<text>Perform the upgrade. [Cost: 35~]</text>
		<event>
			<text>You install the upgrade.</text>
			<item_modify>
				<item type="scrap" min="-35" max="-35" />
			</item_modify>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_CLONEBAY_NOLOSS" />
			</choice>
		</event>
	</choice>
	<choice req="pilot" lvl="1" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>
</event>

<event name="INSTALL_CLONEBAY_NOLOSS">
	<variable name="loc_internal_upgrades" op="add" val="1"/>
</event>

<event name="STORAGE_CHECK_SYSTEM_TELEPORTER_MULTIVERSAL">
	<text>Your elemental scrappers can upgrade your teleporter with multiversal technology.</text>
	<choice req="teleporter" lvl="4" max_lvl="4" blue="true" hidden="true">
		<text>Adamantine Backup Capacitors [Cost: 75~, 5|]</text>
		<event load="MULTIVERSAL_INSTALL_CAPACITOR"/>
	</choice>
	<choice req="teleporter" lvl="5" blue="true" hidden="true">
		<text>Adamantine Backup Capacitors [Installed]</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="UPG_MULTIVERSAL_TELEPORTER_AMPLIFIER" lvl="1" blue="true" hidden="true">
		<text>Multispectral Focal Amplifier [Installed]</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="UPG_MULTIVERSAL_TELEPORTER_AMPLIFIER" lvl="0" max_lvl="0" blue="true" hidden="true">
		<text>Multispectral Focal Amplifier [Cost: 90~, 5|, 4}, 4{]</text>
		<event load="MULTIVERSAL_INSTALL_AMPLIFIER"/>
	</choice>
	<choice req="pilot" lvl="1" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>	
</event>

<event name="MULTIVERSAL_INSTALL_AMPLIFIER">
	<text>Your elemental scrapper can interface with your teleporter and install a modified focal amplifier that coats your boarders in multiversal energy.
	[Effects: When the teleporter is fully powered your boarders gain 25% combat damage and a 35% increase to sabotage speed.]</text>
	<choice req="teleporter" lvl="5" max_group="0" blue="false">
		<text>Perform the upgrade. [Cost: 90~, 5|, 4}, 4{]</text>
		<event>
			<text>You install the upgrade.</text>
			<item_modify>
				<item type="drones" min="-5" max="-5" />
				<item type="scrap" min="-90" max="-90" />
				<item type="missiles" min="-4" max="-4" />
				<item type="fuel" min="-4" max="-4" />
			</item_modify>
			<choice>
				<text>Continue...</text>
				<event load="INSTALL_MULTIVERSAL_AMPLIFIER" />
			</choice>
		</event>
	</choice>
	<choice req="teleporter" lvl="1" max_lvl="4" max_group="1" blue="false" hidden="true">
		<text>This upgrade requires Teleporter Level 5</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="pilot" lvl="1" max_group="2" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>
</event>

<event name="INSTALL_MULTIVERSAL_AMPLIFIER">
	<hiddenAug>UPG_MULTIVERSAL_TELEPORTER_AMPLIFIER</hiddenAug>
	<loadEvent>STORAGE_CHECK_SYSTEM</loadEvent>
</event>


<event name="MULTIVERSAL_INSTALL_CAPACITOR">
	<text>Your elemental scrapper can interface with your teleporter and augment the flux-capacitors with Adamantine Multiversal energy.
	[Effects: Instant Cooldown, is compatible with Reconstructive Teleporter and all other Teleporter upgrades]</text>
	<choice req="teleporter" lvl="4" max_group="0" blue="false" hidden="true">
		<text>Perform the upgrade. [Cost: 75~, 5|]</text>
		<event>
			<text>You install the upgrade.</text>
			<upgrade amount="1" system="teleporter" />
			<item_modify>
				<item type="drones" min="-5" max="-5" />
				<item type="scrap" min="-75" max="-75" />
			</item_modify>
			<choice>
				<text>Continue...</text>
				<event load="STORAGE_CHECK_SYSTEM_LOAD" />
			</choice>
		</event>
	</choice>
	<choice req="pilot" lvl="1" max_group="1" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>
</event>


