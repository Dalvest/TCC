<!-- Enables command console in game \\ press L to use

EVENT          Run an event.                           Usage: EVENT <event_id>        Example: EVENT FREE_AUG
SECTOR         Go to sector type.                      Usage: SECTOR <sector_type>    Example: SECTOR PIRATE_SECTOR
SCRAP          Add scrap.                              Usage: SCRAP <num_to_add>      Example: SCRAP 500
WEAPON         Add a weapon blueprint.                 Usage: WEAPON <weapon_id>      Example: WEAPON LASER_BURST_1
DRONES         Add drone parts.                        Usage: DRONES <num_to_add>     Example: DRONES 50
RICH           Add 80 of each resource and 1999 scrap. Usage: RICH
GOD            Fully upgrade every system and reactor. Usage: GOD
DELETE         Destroys enemy ship.                    Usage: DELETE
SYS            Add a system.                           Usage: SYS <system_name>       Example: SYS cloaking
AUG            Add an augmentation.                    Usage: AUG <augment_name>      Example: AUG O2_MASKS
CREW           Add a crew member.                      Usage: CREW [race_name]        Example: CREW slug
KILL           Kills a person on your ship             Usage: KILL <crew_name>        Example: KILL John Wick
DELETECREW     Kills everyone on the enemy ship        Usage: DELETECREW
SPEED          Changes gamespeed
-->
<FTL>
  <mod:findLike type="scripts">
    <mod-append:script>data/tcc_scripts/dependency.lua</mod-append:script>
  </mod:findLike>
</FTL>
<FTL><!--appending to Inferno-Core-->
	<events>
		<variable name="tcc_called_compactor" op="set" val="0"/>
		<variable name="tc_call_marker" op="set" val="0"/>
		<req name="TCC_CAN_CALL_COMPACTOR" type="all">
			<req>
				<name>SCRAP_TRANSMITTER</name>
			</req>
			<req lvl="0" max_lvl="0">
				<name>tcc_called_compactor</name>
        <name>tc_call_marker</name>
			</req>
		</req>
	</events>

	<mod:findLike type="events">
		<mod:findName type="req" name="FMCORE_LIST_JUMPSPACE_EQUIPMENT">
			<mod-append:name>SCRAP_TRANSMITTER</mod-append:name>
		</mod:findName>
	</mod:findLike>
</FTL>
<FTL>
  <mod:findLike type="events">
    <mod:findName type="event" name="START_BEACON">
      <mod-append:hiddenAug>ADDON_INSTALLED_TCC</mod-append:hiddenAug>
    </mod:findName>
  </mod:findLike>
  <events><!--i am 99% sure that those reqs couldn't work bc missing <events>-->
    <req name="SCAVENGER_REQ" type="all">
      <name>ADVANCED_SCRAP_REQ</name>
      <name>LIST_CHECKCREW_SCRAPPER_ELITE_COMPATIBLE</name>
    </req>
	<variable name="tc_found_echelon" op="set" val="0"/>

    <req name="ADVANCED_SCRAP_REQ" type="any">
      <name>ADVANCED_SCRAP</name>
      <name>trash_eater</name>
    </req>
	
    <req name="TC_TRASH_FIRSTMEET" type="any" max_lvl="0">
      <name>tc_trash_met</name>
    </req>
	
    <req name="TC_SCRAPPER_JOB_REQ" type="sum">
        <name>tc_scrapper_job</name>
		<req mult="-999" max_lvl="0">
			<req type="sum" load="LIST_CREW_SCRAPPER"/>
		</req>
    </req>

    <!--///////////////AVALI EVENT STUFF/////////-->
    <event name="INSTALL_LAB_AVALI_DENSEFLUFF">
      <hiddenAug>LAB_AVALI_DENSEFLUFF</hiddenAug>
      <hiddenAug>LAB_AVALI_INSTALLED</hiddenAug>
      <loadEvent>STORAGE_CHECK_LAB_AVALI</loadEvent>
    </event>
    <event name="INSTALL_LAB_AVALI_FLAMECLOAKS">
      <hiddenAug>LAB_AVALI_FLAMECLOAKS</hiddenAug>
      <hiddenAug>LAB_AVALI_INSTALLED</hiddenAug>
      <loadEvent>STORAGE_CHECK_LAB_AVALI</loadEvent>
    </event>
    <event name="INSTALL_LAB_AVALI_CYBERNETIC">
      <!--pardon the naming scheme, enhanced cybernetics used to be a lab, and no clone XP was an exclusive aug to avali C, but now its external enhanced cybernetics-->
      <hiddenAug>LAB_AVALI_NOCLONEXP</hiddenAug>
      <hiddenAug>LAB_AVALI_INSTALLED</hiddenAug>
      <loadEvent>STORAGE_CHECK_LAB_AVALI</loadEvent>
    </event>
    <bossShip yOffset="90">MU_BOSS_ENGINEER_C</bossShip>
    <!--sector stuff-->
    <eventFile>avali</eventFile>
	  <eventFile>rsa</eventFile>
    <eventFile>echelon</eventFile>
  </events>

