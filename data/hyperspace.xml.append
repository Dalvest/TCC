</FTL>
<FTL><!--appending to Inferno-Core-->
	<events>
		<variable name="tcc_called_compactor" op="set" val="0"/>
		<variable name="tc_call_marker" op="set" val="0"/>
		<req name="TCC_CAN_CALL_COMPACTOR" type="all">
			<req>
				<name>SCRAP_TRANSMITTER</name>
			</req>
			<req lvl="0" max_lvl="0">
				<name>tcc_called_compactor</name>
        <name>tc_call_marker</name>
			</req>
		</req>
	</events>

	<mod:findLike type="events">
		<mod:findName type="req" name="FMCORE_LIST_JUMPSPACE_EQUIPMENT">
			<mod-append:name>SCRAP_TRANSMITTER</mod-append:name>
		</mod:findName>
	</mod:findLike>
</FTL>
<FTL>
  <mod:findLike type="events">
