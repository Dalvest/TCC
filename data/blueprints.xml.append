	<weaponBlueprint name="TC_DEBUFF_SLOW_1">
		<type>LASER</type>
		<title>Debuffer (Movement)</title>
		<short>Debuffer (M)</short>
		<desc>This laser halves the speed of all enemy crew on the enemy ship. Stacks infinitely for now.</desc>
		<tooltip>WIP Laser, no damage, cannot miss, 1 second cooldown, 10 shield pierce, slow.</tooltip>
		<damage>0</damage>
		<accuracyMod>100</accuracyMod>
		<!--STATBOOST THINGS-->
		<spawnCrew>
			<race>tcc_generic_crewjank</race>
			<number>1</number>
			<enemy>true</enemy><!--So you don't ever see the crew-->
			<lifetime>0</lifetime>
			<noSlot>true</noSlot>
			<statBoosts>

				<statBoost name="deathEffect"><!--This modifies the stat of the crew, so that the same crew can be used for multiple weapons-->
					<boostType>SET</boostType>


					<deathEffect><!--When template crew dies (on weapon impact) these statBoosts are applied -->
						<statBoosts>
							<statBoost name="moveSpeedMultiplier"><!--Speed stat-->
								<boostType>MULT</boostType><!--MULT and 0.5 multiplies the value of the stat by 0.5, so crew speed is halved-->
								<amount>0.5</amount>
								<shipTarget>CURRENT_ALL</shipTarget><!--Affects all rooms in the current ship-->
								<crewTarget>ORIGINAL_ALLIES</crewTarget><!--Targets allies OF THE SPAWNED CREW, so your enemeis. ORIGINAL_ALLIES ignores mind control-->
								<duration>5</duration><!--This statBoost lingers for 5 seconds-->
								<boostAnim>bio_poison</boostAnim><!--The bio_poison anim will play over all affected crew-->
								<maxStacks id="tcc_lingering_slow_1">999</maxStacks><!--The ID makes it so the stacks of THIS boost are counted, so even if stacks are limited it can still stack with OTHER speed-altering affects-->
								<!--999 means it can self-stack 999 times, so effectively infintely. I left this in however, so the value can be changed later.-->
								<blackList><!--To prevent the animation from playing over the template crew-->
									<tcc_generic_crewjank/>
								</blackList>
							</statBoost>
						</statBoosts>
					</deathEffect>

					<shipTarget>ALL</shipTarget>
					<crewTarget>ALL</crewTarget>
					<priority>9999999</priority>
				</statBoost>


			</statBoosts>
		</spawnCrew>

